#-----------------------------------------------------------------------------
# h5zstd Version file for install directory
#-----------------------------------------------------------------------------

set (PACKAGE_VERSION @H5ZSTD_VERSION_STRING@)

if ("${PACKAGE_FIND_VERSION_MAJOR}" EQUAL @H5ZSTD_VERSION_MAJOR@)

  # exact match for version @H5ZSTD_VERSION_MAJOR@.@H5ZSTD_VERSION_MINOR@
  if ("${PACKAGE_FIND_VERSION_MINOR}" EQUAL @H5ZSTD_VERSION_MINOR@)

    # compatible with any version @H5ZSTD_VERSION_MAJOR@.@H5ZSTD_VERSION_MINOR@.x
    set (PACKAGE_VERSION_COMPATIBLE 1)

    if ("${PACKAGE_FIND_VERSION_PATCH}" EQUAL @H5ZSTD_VERSION_RELEASE@)
      set (PACKAGE_VERSION_EXACT 1)

      if ("${PACKAGE_FIND_VERSION_TWEAK}" EQUAL @H5ZSTD_VERSION_SUBRELEASE@)
        # not using this yet
      endif ()

    endif ()

  endif ()
endif ()


